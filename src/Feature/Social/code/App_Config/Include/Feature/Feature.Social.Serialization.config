<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Social" description="Feature Social" dependencies="Foundation.*" extends="Helix.Feature">
            <predicate>
            <include name="Feature.Social.Media" database="master" path="/sitecore/media library/Feature/Social" />
          </predicate>
            <rolePredicate>
            <include domain="modules" pattern="^Feature Social .*$" />
          </rolePredicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>