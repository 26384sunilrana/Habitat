<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Person" description="Feature Person" dependencies="Foundation.*" extends="Helix.Feature">
            <predicate>
            <include name="Feature.Person.Media" database="master" path="/sitecore/media library/Feature/Person" />
          </predicate>
            <rolePredicate>
            <include domain="modules" pattern="^Feature Person .*$" />
          </rolePredicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>