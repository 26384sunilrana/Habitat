<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Multisite" description="Feature Multisite" dependencies="Foundation.*" extends="Helix.Feature">
            <predicate>
            <include name="Feature.Multisite.Media" database="master" path="/sitecore/media library/Feature/Multisite" />
          </predicate>
            <rolePredicate>
            <include domain="modules" pattern="^Feature Multisite .*$" />
          </rolePredicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>