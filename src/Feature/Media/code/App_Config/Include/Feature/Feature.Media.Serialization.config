<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Media" description="Feature Media" dependencies="Foundation.*" extends="Helix.Feature">
            <predicate>
            <include name="Feature.Media.Settings" database="master" path="/sitecore/system/Settings/Feature/Media" />
            <include name="Feature.Media.Media" database="master" path="/sitecore/media library/Feature/Media" />
          </predicate>
            <rolePredicate>
            <include domain="modules" pattern="^Feature Media .*$" />
          </rolePredicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>