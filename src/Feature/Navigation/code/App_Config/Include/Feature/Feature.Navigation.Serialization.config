<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Navigation" description="Feature Navigation" dependencies="Foundation.*" extends="Helix.Feature">
            <predicate>
            <include name="Feature.Navigation.Media" database="master" path="/sitecore/media library/Feature/Navigation" />
          </predicate>
            <rolePredicate>
            <include domain="modules" pattern="^Feature Navigation .*$" />
          </rolePredicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>