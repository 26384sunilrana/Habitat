@using Habitat.Framework.SitecoreExtensions.Extensions
@using Sitecore
@using Constants = Habitat.Website.Constants
@model Sitecore.Mvc.Presentation.RenderingModel
@{ var useStaticPlaceholderNames = MainUtil.GetBool(Model.Rendering.Parameters[Constants.LayoutParameters.UseStaticPlaceholderNames], false);}
<div class="container@(MainUtil.GetBool(Model.Rendering.Parameters[Constants.LayoutParameters.IsFluid], false) ? "-fluid" : "" )">
    <div class="row">
        <div class="col-md-offset-4 col-md-4">
            @Html.Sitecore().DynamicPlaceholder("col-centered-1", useStaticPlaceholderNames)
        </div>
    </div>
</div>
